set ttyfast

set noeb vb t_vb=
set nocompatible

set hidden

set encoding=utf-8
set updatetime=300
set signcolumn=yes

set number
set nobackup
set nowritebackup
set clipboard=unnamed

set smartcase
set incsearch
set ignorecase
set showmatch

set history=1000

set nowrap
set tabstop=2
set shiftwidth=2
set softtabstop=2
set expandtab
set noshiftround

set wildmenu
set wildmode=full
